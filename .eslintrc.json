{
    "extends": ["airbnb", "plugin:react/recommended"],
    "env": {
        "browser": true
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "max-len": 0,
        "linebreak-style": 0,
        "no-console": 0,
        "react/forbid-prop-types": 0,
        "react/prop-types": 0,
        "react/destructuring-assignment": 0,
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
    }
}